// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("SUPABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Course {
  code_cours    String  @id
  nom_cours     String
  professeur    String
  description   String
}

model Unit {
  id        Int     @id @default(autoincrement())
  full_name String
  name      String
  code      Int
}
